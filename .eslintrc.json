{
    "env": {
        "browser": false,
        "es6": false
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "error",
        "quotes": "error",
        "object-curly-spacing": ["error", "always"],
        "no-mixed-spaces-and-tabs": "error"
    }
}
